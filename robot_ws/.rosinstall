- git: {local-name: src/deps/cloudwatch-common, uri: 'ssh://github.com/aws-robotics/cloudwatch-common', version: mainline}
- git: {local-name: src/deps/cloudwatchlogs-ros1, uri: 'ssh://github.com/aws-robotics/cloudwatchlogs-ros1', version: mainline}
- git: {local-name: src/deps/cloudwatchmetrics-ros1, uri: 'ssh://github.com/aws-robotics/cloudwatchmetrics-ros1', version: mainline}
- git: {local-name: src/deps/health-metrics-collector-ros1, uri: 'ssh://github.com/aws-robotics/health-metrics-collector-ros1', version: mainline}
